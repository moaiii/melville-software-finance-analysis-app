{
  "extends": "skyscanner",
  "rules": {
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 1,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "operator-linebreak": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-curly-brace-presence": 0,
    "react/jsx-wrap-multilines": 0,
    "react/no-array-index-key": 0,
    "backpack/use-components": 0,
    "react/prop-types": 0
  },
  "env": {
    "jest": true
}
}